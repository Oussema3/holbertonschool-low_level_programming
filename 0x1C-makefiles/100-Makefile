# complete advanced makefile HA HA 
SRC = main.c holberton.c
CC = gcc 
OBJ = $(SRC:.c=.o)
NAME = holberton 
CFLAGS  = -Wall -Werror -Wextra -pedantic
.PHONY: all clean oclean fclean re

all: m.h $(OBJ)
	$(CC) $(OBJ) -o $(NAME)
clean:
	$(RM) -f *~ $(NAME)
occlean:
	$(RM) -f $(OBJ)
fclean: clean oclean
re: oclean all
